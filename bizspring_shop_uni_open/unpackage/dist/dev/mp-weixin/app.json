{
  "pages": [
    "pages/index/index",
    "pages/goods_category/index",
    "pages/cart/list",
    "pages/member/index",
    "pages/utils/agreement"
  ],
  "subPackages": [
    {
      "root": "order",
      "pages": [
        "order/checkout",
        "order/payment",
        "order/list"
      ]
    },
    {
      "root": "goods",
      "pages": [
        "goods/detail",
        "goods/list"
      ]
    },
    {
      "root": "store",
      "pages": [
        "store/index"
      ]
    },
    {
      "root": "upms",
      "pages": [
        "login/index",
        "address/address",
        "address/address_add",
        "goods_favorite/list",
        "store_favorite/list"
      ]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTitleText": "商城",
    "navigationBarTextStyle": "black"
  },
  "tabBar": {
    "color": "#858585",
    "selectedColor": "#fd462f",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "/static/images/icons/btn-tabs_01.png",
        "selectedIconPath": "/static/images/icons/btn-tabs_07.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/goods_category/index",
        "iconPath": "/static/images/icons/btn-tabs_03.png",
        "selectedIconPath": "/static/images/icons/btn-tabs_08.png",
        "text": "分类"
      },
      {
        "pagePath": "pages/cart/list",
        "iconPath": "/static/images/icons/btn-tabs_05.png",
        "selectedIconPath": "/static/images/icons/btn-tabs_09.png",
        "text": "购物车"
      },
      {
        "pagePath": "pages/member/index",
        "iconPath": "/static/images/icons/btn-tabs_06.png",
        "selectedIconPath": "/static/images/icons/btn-tabs_10.png",
        "text": "我的"
      }
    ]
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "__APP__"
      ]
    }
  },
  "networkTimeout": {
    "request": 6000000
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "plugins": {},
  "usingComponents": {}
}